<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--   color for the app bar and other primary UI elements -->
    <color name="colorPrimary">@color/Indigo500</color>
    <!--   a darker variant of the primary color, used for
           the status bar (on Android 5.0+) and contextual app bars -->
    <color name="colorPrimaryDark">@color/Indigo700</color>
    <!--   a secondary color for controls like checkboxes and text fields -->
    <color name="colorAccent">#FF4081</color>

    <color name="basicTextViewTextColor">@android:color/black</color>
    <color name="basicEditTextViewTextColor">@android:color/white</color>
    <color name="basicTextHintColor">@color/Gray500</color>
    <color name="basicBarcodeScanColor">@android:color/white</color>
    <color name="basicTableColor">@color/Gray200</color>
</resources>